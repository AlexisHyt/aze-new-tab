{
	"name": "aze-new-tab",
	"displayName": "ANT - Custom new tab",
	"version": "2.0.0",
	"description": "Customize your new tab page with your shortcuts and your RSS feeds.",
	"author": "AlexisH <azen0x.alexis@gmail.com>",
	"scripts": {
		"dev": "plasmo dev",
		"build": "plasmo build",
		"package": "plasmo package",
		"biome:check": "pnpm exec biome check",
		"biome:fix": "pnpm exec biome check --write",
		"biome:format": "pnpm exec biome format",
		"biome:lint": "pnpm exec biome lint"
	},
	"dependencies": {
		"@flyyer/use-googlefonts": "^0.4.2",
		"@material-tailwind/react": "^2.1.10",
		"@plasmohq/storage": "^1.15.0",
		"color": "^5.0.2",
		"plasmo": "0.90.5",
		"react": "18.2.0",
		"react-color": "^2.19.3",
		"react-dom": "18.2.0",
		"react-icons": "^5.5.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@ianvs/prettier-plugin-sort-imports": "4.1.1",
		"@types/chrome": "0.0.258",
		"@types/node": "20.11.5",
		"@types/react": "18.2.48",
		"@types/react-dom": "18.2.18",
		"autoprefixer": "^10.4.21",
		"crypto-browserify": "^3.12.0",
		"postcss": "^8.5.6",
		"prettier": "3.2.4",
		"tailwindcss": "3",
		"typescript": "5.3.3"
	},
	"manifest": {
		"permissions": [
			"storage"
		],
		"host_permissions": [
			"https://*/*",
			"http://*/*"
		]
	}
}
